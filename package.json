{
  "name": "skeleton",
  "version": "3.0.0",
  "description": "HTML CSS スターターキット",
  "main": "index.js",
  "scripts": {
    "rscss": "node index.js recursiveSassCompile",
    "scss": "node-sass src/sass/style.scss dist/assets/css/style.css -w --output-style compressed --source-map true",
    "rollup": "rollup --config rollup.config.js -w -m --no-strict",
    "start": "npm run scss & npm run rollup"
  },
