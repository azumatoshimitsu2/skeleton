  "bugs": {
    "url": "https://github.com/azumatoshimitsu2/skeleton/issues"
  },
  "homepage": "https://github.com/azumatoshimitsu2/skeleton#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-preset-es2015-rollup": "^3.0.0",
    "chokidar": "^3.0.2",
    "fs-extra": "^8.1.0",
    "node-sass": "^4.12.0",
    "rollup": "^1.12.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-notify": "^1.1.0",
